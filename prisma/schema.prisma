generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model corp_comments {
  id          String   @id @default(auto()) @map("_id") @db.ObjectId
  comment     String
  companyName String
  createdAt   DateTime @db.Date
  updatedAt   DateTime @db.Date
}
